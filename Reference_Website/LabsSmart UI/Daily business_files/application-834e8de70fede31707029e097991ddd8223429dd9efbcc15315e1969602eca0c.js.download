$(document).on('turbolinks:load', function() {

  function ignoreDotAndSpace(params, data) {
    params.term = params.term || '';

    term = params.term.toUpperCase();
    text = data.text.toUpperCase();
    textWithoutSpaceDot = text.replace(/\./g, '').replace(/\s+/g, '');

    // allow exact matches or matches without space and dot
    if ( (text.indexOf(term) > -1 ) || (textWithoutSpaceDot.indexOf(term) > -1 )) {
      return data;
    }
    return false;
  }

  function resetModalContent(modal) {
    var loadingHtml = '<p>Loading...</p>';
    var contentArea = modal.children('#ls-modal-content-area');
    modal.removeClass('large');
    contentArea.html( loadingHtml );
  }

  function siblingSelectedOptionValue(ele){
    var val = ele.siblings('div.select').children('select').children("option:selected").val();
    return val;
  }

  function findUrl(ele){
    var link = ele.data('link');
    var id = siblingSelectedOptionValue(ele);
    if (id.length > 0) {
      return link.replace("__ID__", id);
    } else {
      return link;
    }
  }

  function invalidUrl(url) {
    return /__ID__/.test(url);
  }

  function disableEnableLink() {
    var anchor_tag = $('a.ls-modal-edit-link');
    var selectedValue = siblingSelectedOptionValue(anchor_tag);
    if(selectedValue.length == 0){
      anchor_tag.addClass('disabled-link');
    } else {
      anchor_tag.removeClass('disabled-link');
    }
  }

  // Run disable link only when link is present on the page
  if($('a.ls-modal-edit-link').length > 0) {
    disableEnableLink();
  }

  $('select.front-end-editable').change(function () {
    disableEnableLink();
  });

  function populateModal(url, modalSize, modal) {
    resetModalContent(modal);
    modal.addClass(modalSize);
    modal.foundation('open');
    $.get( url, function( data ) {
      var contentArea = modal.children('#ls-modal-content-area');
      contentArea.html( data );
      $('#ls-modal-content-area .select2').select2({
        dropdownParent: $('#ls-modal-content-area'),
        matcher: function(params, data) {
            return ignoreDotAndSpace(params, data);
        }
      }); // initialize custom select2 on select tags
      $(document).foundation(); // ensure foundation js works on loaded data, eg. Tabs UI
    });
  }

  $('.ls-modal-link, .ls-modal-new-link').click(function (e) {
    e.preventDefault();
    var url = $(this).data('link');
    var modalSize = $(this).data('modal-size');
    var modal = $('#ls-modal');
    populateModal(url, modalSize, modal);
  });

  $('.ls-modal-edit-link').click(function (e) {
    e.preventDefault();
    var url = findUrl($(this));
    var modalSize = $(this).data('modal-size');
    if (invalidUrl(url)) { return; }
    var modal = $('#ls-modal');
    populateModal(url, modalSize, modal);
  });

    // update lab ratelist
    $('.select2-ignore-dots-spaces.investigation_ids.lab_case').on('select2:select', function (e) {
        var data = e.params.data;
        if (data.element.dataset['seedPrice'] === 'false') {
            return; // Do nothing if not seeded price
        }
        var labIdentifier = $('#lab_identifier').val();
        var url = '/' + labIdentifier + '/lab_case_investigations/' + data.id + '/edit?layout=false';
        var modal = $('#ls-modal');
        populateModal(url, 'large', modal);
    });

    // update other ratelist
    // select all cases but lab case
    $('.select2-ignore-dots-spaces.investigation_ids:not(.lab_case)').on('select2:select', function (e) {
        var data = e.params.data;
        if (data.element.dataset['seedPrice'] === 'false') {
            return; // Do nothing if not seeded price
        }
        var labIdentifier = $('#lab_identifier').val();
        var url = '/' + labIdentifier + '/investigations/' + data.id + '/edit?layout=false';
        var modal = $('#ls-modal');
        populateModal(url, 'large', modal);
    });
});
