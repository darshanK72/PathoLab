    if(inRange){
      resetTestResultHighlight(ele);
    }
    else if(result > upperValue){
      ele.css("font-weight", "bold");
      ele.closest('tr').css("font-weight", "bold");
      ele.closest('tr').find('span.H-and-L-marker').text('H');
