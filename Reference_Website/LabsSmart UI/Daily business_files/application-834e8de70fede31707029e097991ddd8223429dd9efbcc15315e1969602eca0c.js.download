      $.post( url, function() { });
      callPoll(pollUrl);
    }
  });

  function callPoll(url) {
    if ($('#download-usg-report-pdf').attr('href') === '#') {
      pollPdfUrl(url);
    }
  }

  function pollPdfUrl(url) {
    setTimeout(function() {
        $.ajax({
            url: url,
            type: "GET",
            success: function(data) {
                console.log(data.pdf_url);
                if (data.pdf_url !== null) {
                  $('#download-usg-report-pdf').prop('href', data.pdf_url).unbind('click');
                  $('#pdf-url-loader').addClass('hide');
                }
            },
            dataType: "json",
            // Start first call after 3 seconds and 100ms.
            // Avoids redundant call to server, adds extra 3 sec wait.
            complete: setTimeout(function(){ callPoll(url); }, 3000),
            timeout: 15000
        })
    }, 100);
  }
});
