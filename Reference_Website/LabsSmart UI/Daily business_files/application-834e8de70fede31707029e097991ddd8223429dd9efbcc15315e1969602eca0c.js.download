$(document).on('turbolinks:load', function() {

  var insertImageLock = 'off';

  // NOTE - Using css from usg_reports.scss, class - usg_reports content-rich_text
  // NOTE - These css is written to fix the issue when you copy paste tables from MS-Word into TinyMCE editor.
  function tinymceEditorCss() {
    return `
      .mce-item-table, .mce-item-table caption, .mce-item-table td, .mce-item-table th {
        border: 1px solid #6b7280;
      }
      .mce-item-table td {
        padding: 2px 5px;
      }
      .mce-item-table td p {
        margin: 0;
        font-size: 14px;
      }
    `;
  }

  function showUploadProgress() {
    html = `<div id="uploadProgressIndicator" class="fixed bg-blue-200 border border-gray-200 shadow-2xl p-2 rounded-xl flex items-center justify-center" style="z-index: 1052; left: 50%; top: 44%;">
              <div class="inline-flex items-center gap-2 text-gray-700 font-semibold text-base">
                <img class="h-12 w-12" src="https://res.cloudinary.com/https-labsmartlis-com/image/upload/v1734350716/uploader-gif_eyg8lx.gif">
                Please wait. Uploading the image...
              </div>
            </div>`;
    $('body').append(html)
  }

  function hideUploadProgress() {
    $('div#uploadProgressIndicator').remove()
  }

  function uploadImage(cfUploadUrl, file, editor) {
    var formData = new FormData()
    formData.append('file', file)

    fetch(cfUploadUrl, {
      method: 'POST',
      body: formData
    }).then((response) => response.json())
      .then((data) => { processResponse(data, editor); });
  }

  function processResponse(data, editor){
    // var cf_image_id = data['result']['id'];
    var imgSrc = data.result.variants[0];
    editor.insertContent(`<img src=${imgSrc} />`);
    hideUploadProgress();
    insertImageLock = 'off'
  }

  function cfUploadImage(editor) {
    const fileInput = document.createElement('input');
    fileInput.type = 'file';
    fileInput.accept = 'image/*'; // Restrict to image files only

    // Listen for the file selection
    fileInput.onchange = function (event) {
      const file = event.target.files[0]; // Get the selected file
      if (file) {
        if (insertImageLock === 'on') {
          alert('Image upload already in progress, please wait.')
        } else {
          showUploadProgress();
          insertImageLock = 'on'
          var url = '/cf_upload_url'
          fetch(url, {
            method: 'POST',
            headers: {
              'Accept': 'application/json', // Accept is used by rails to determine request format.
              'Content-Type': 'application/json;charset=utf-8'
            },
          }).then((response) => response.json())
            .then((data) => { uploadImage(data['upload_url'], file, editor) });
        }
      }
    };

    // Trigger the file input click event
    fileInput.click();
  }

  $('textarea.tinymce').tinymce({
    plugins: "table",
    menubar: false,
    toolbar: "table",
    browser_spellcheck: true
  });

  $('textarea.full-featured-tinymce').tinymce({
    block_formats: 'Paragraph=p;Header 3=h3;Header 4=h4',
    content_style: tinymceEditorCss(),
    plugins: "table lists paste autoresize pagebreak",
    autoresize_bottom_margin: 50,
    max_height: 600,
    menubar: false,
    font_formats: "Andale Mono=andale mono,times; Arial=arial,helvetica,sans-serif; Arial Black=arial black,avant garde; Book Antiqua=book antiqua,palatino; Courier New=courier new,courier; Georgia=georgia,palatino; Helvetica=helvetica; Impact=impact,chicago; Symbol=symbol; Tahoma=tahoma,arial,helvetica,sans-serif; Terminal=terminal,monaco; Times New Roman=times new roman,times; Trebuchet MS=trebuchet ms,geneva; Verdana=verdana,geneva;",
    fontsize_formats: '8pt 10pt 12pt 14pt 16pt 18pt 24pt',
    toolbar: "removeformat | insertImageButton | pagebreak | fontselect formatselect fontsizeselect | undo redo | bold | table | alignleft aligncenter alignright alignjustify | bullist numlist",
    setup: function (editor) {
      editor.ui.registry.addButton('insertImageButton', {
        text: 'Insert image', // Button text
        icon: 'image',
        tooltip: 'Upload and insert image', // Tooltip on hover
        onAction: function () {
          cfUploadImage(editor);
          // editor.insertContent(`<img src=${src} />`);
        },
      });
    },
    pagebreak_separator: '<div style="page-break-before: always;">',
    browser_spellcheck: true,
  });

