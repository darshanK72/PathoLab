$(document).on('turbolinks:load', function() {

  const eGFRCalculate = (sex, serumCreatinine, age) => {
    K = (sex == "M" ? 0.9 : 0.7)
    alpha = (sex == "M" ? -0.302 : -0.241)
    if((serumCreatinine/K)>1) {
        min = 1
        max = serumCreatinine/K
    } else {
        min = serumCreatinine/K
        max = 1
    }
    constant = (sex == "M" ? 1 : 1.012)
    eGFR = 142 * Math.pow(min, alpha) * Math.pow(max, -1.200) * Math.pow(0.9938,age) * constant
    return eGFR.toFixed(2)
  }

  function seteGFR(serumCreatinine) {
    var sex = document.getElementById('patient_sex').value
    var age = Number(document.getElementById('patient_age_in_year').value)
    var result = eGFRCalculate(sex, serumCreatinine, age)
    $('.eGFR').val(result).change()
  }

  $(".serum-creatinine").on('change', function(){
    var serumCreatinine = Number($(this).val()?.replaceAll(",", ""))
    seteGFR(serumCreatinine);
  });

  // NOTE -
  // G1 >= 90
  // G2 60-89
  // G3a 45-59
  // G3b 30-44
  // G4 15-29
  // G5 < 15
  function seteGFRcategory(eGFRValue) {
    switch (true) {
      case (eGFRValue >= 0 && eGFRValue < 15):
        $('.eGFR-cateogry').val('G5');
        break;
      case (eGFRValue >= 15 && eGFRValue <= 29):
        $('.eGFR-cateogry').val('G4');
        break;
      case (eGFRValue >= 30 && eGFRValue <= 44):
        $('.eGFR-cateogry').val('G3b');
        break;
      case (eGFRValue >= 45 && eGFRValue <= 59):
        $('.eGFR-cateogry').val('G3a');
        break;
      case (eGFRValue >= 60 && eGFRValue <= 89):
        $('.eGFR-cateogry').val('G2');
        break;
      case (eGFRValue >= 90):
        $('.eGFR-cateogry').val('G1');
        break;
    }
  }

  $('.eGFR').on('change', function(){
    var eGFRValue = Number($(this).val()?.replaceAll(",", ""))
    seteGFRcategory(eGFRValue);
  });

});
