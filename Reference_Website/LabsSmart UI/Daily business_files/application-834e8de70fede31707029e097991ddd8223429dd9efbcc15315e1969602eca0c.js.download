  function resetTestResultHighlight(ele) {
    ele.css("font-weight", "normal");
    ele.closest('tr').css("font-weight", "normal");
    ele.closest('tr').find('span.H-and-L-marker').text('');
  }

  function highlightTestResults(ele){
    var lowerValue = +ele.data('lower-value');
    var upperValue = +ele.data('upper-value');
    var result = +ele.val().replace('>', '').replace('<', '').replaceAll(",", ""); // allow having gt and lt symbols

