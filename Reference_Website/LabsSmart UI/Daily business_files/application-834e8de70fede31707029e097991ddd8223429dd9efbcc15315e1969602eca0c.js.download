$(document).on('turbolinks:load', function() {
  updateReceiptAmount();
  updateTotalDiscount();

  function updateReceiptAmount() {
    var totalFeePaid = 0;
    $('.caseable_fields .fee_paid').each(function () {
      var feePaid = Number($(this).val());
      if (!isNaN(feePaid)) {
        totalFeePaid += (feePaid);
      }
    });
    var amountAlreadyPaid = $('#amount-already-paid').text();
    var totalFeeToPay = totalFeePaid - amountAlreadyPaid;

    if (totalFeeToPay < 0) {
      $('#case_receipts_attributes_0_receipt_type').val('refund');
      $('a.accept-due').hide();
      $('.case_receipts_collection_charge_paid').hide();
      $('#case_receipts_attributes_0_collection_charge_paid').val(0);
      $('.receipt_fields #amount-received').val(- totalFeeToPay);
      $('.receipt-amount-label label').text('Amount refund');
      $('.receipt-amount-label label').css("color", "red");
    } else {
      $('#case_receipts_attributes_0_receipt_type').val('income');
      $('a.accept-due').show();
      $('.case_receipts_collection_charge_paid').show();
      $('.receipt_fields #amount-received').val(totalFeeToPay);
      $('.receipt-amount-label label').text('Amount received');
      $('.receipt-amount-label label').css("color", "");
    }
  }

  function updateTotalDiscount() {
    var sum = 0;
    $('.discount').each(function () {
      sum += Number($(this).val());
    });
    $('input#total-discount').val(sum);
  }

  $('.tab-title, .add-caseable-fields').click(function (e) {
    e.preventDefault();
    var openDivId = $(this).data('open-div-id');
    $('#' + openDivId).removeClass('hide');
    if(openDivId === 'lab_case_fields' || openDivId === 'outsource_lab_case_fields') {
      $('#collection-charge').removeClass('hide');
    }
    $('.select2').width('100%');
  });

  $('.caseable_fields .hide-caseable-fields').click(function(e){
    e.preventDefault();
    var caseType = $(this).data('case-type');
    if (caseType == 'LabCase') {
      $('#collection-charge input').val(0);
      $('#collection-charge').addClass('hide');
    }
    $(this).closest('.caseable_fields').find('.discount').val(0);
    $(this).closest('.caseable_fields').find('.fee_paid').val(0);
    $(this).closest('.caseable_fields').find('.investigation_ids').val([]).trigger("change");
    updateTotalDiscount();
    updateTotalDue();
    $(this).closest('.caseable_fields').addClass('hide');
  });

  // On investigation change Update total fee, fee paid, receipt amount
  $('.caseable_fields .investigation_ids').change(function(){
    var totalFee = 0;
    $(this).find('option:selected').each(function(){
      totalFee += $(this).data('fee');
    });
    var discount = $(this).closest('.caseable_fields').find('.discount').val();
    $(this).closest('.caseable_fields').find('.caseable-total').text(totalFee);
    $(this).closest('.caseable_fields').find('.total_fee').val(totalFee);
    $(this).closest('.caseable_fields').find('.fee_paid').val(totalFee - discount);

    var feePaidEle = $(this).closest('.caseable_fields').find('.fee_paid');
    updateReceiptAmount();
    updateTotalAmount();
    updateCaseableDue($(this).closest('.caseable_fields'));
    updateTotalDue();
  });

  // On discount change Update fee paid
  $('.caseable_fields .discount').on('change paste keyup', function(e){
    if (e.which === 9) return false; // do nothing when keyup event is triggered by tab button
    var totalFee = $(this).closest('.caseable_fields').find('.total_fee').val();
    var discount = $(this).val();
    $(this).closest('.caseable_fields').find('.fee_paid').val(totalFee - discount).trigger('change');

    var feePaidEle = $(this).closest('.caseable_fields').find('.fee_paid');
    updateReceiptAmount();
    updateTotalDiscount();
  });

  // On fee paid change Update Receipt amount and Due
  $('.caseable_fields .fee_paid').on('change paste keyup', function(){
    updateReceiptAmount();
    updateCaseableDue($(this).closest('.caseable_fields'));
    updateTotalDue();
  });

  function updateCaseableDue(caseableFieldsEle) {
    var totalFee = caseableFieldsEle.find('input.total_fee').val();
    var feePaid = caseableFieldsEle.find('input.fee_paid').val();
    var discount = caseableFieldsEle.find('input.discount').val();
    var due = totalFee - feePaid - discount;
    caseableFieldsEle.find('.caseable-due').text(due);
  }

  function updateTotalDue() {
    var sum = 0;
    $('span.caseable-due').each(function () {
      var casebaleCancelEle = $(this).closest('.caseable_fields').find('.cancel-caseable');
      // Do not sum if caseable is canceled.
      if (!casebaleCancelEle.is(":checked")) {
        sum += Number($(this).text());
      }
    });
    $('span#total-due').text(sum);
  }

  function updateTotalAmount() {
    var sum = 0;
    $('span.caseable-total').each(function () {
      sum += Number($(this).text());
    });
    $('span#total-amount').text(sum);
  }

  function newDiscountDistribution(totalDiscount) {
    var discountRemaining = totalDiscount;
    var discountFieldsRemaining = $('.caseable_fields:not(.hide) .discount').length;
    $('.caseable_fields:not(.hide) .discount').each(function () {
      discountFieldsRemaining = discountFieldsRemaining - 1;
      var casebaleDiscountEle = $(this);
      var caseableFeePaidEle = casebaleDiscountEle.closest('.caseable_fields').find('.fee_paid');
      var caseableTotalFee = Number(casebaleDiscountEle.closest('.caseable_fields').find('.total_fee').val());
      var netAmount = Number($('span#total-amount').text());

      if (discountFieldsRemaining == 0) {
        casebaleDiscountEle.val(discountRemaining);
        caseableFeePaidEle.val(caseableTotalFee - discountRemaining);
      } else {
        var discountingRatio = caseableTotalFee / netAmount;
        var discountApplicable = parseInt(discountingRatio * totalDiscount);

        casebaleDiscountEle.val(discountApplicable);
        caseableFeePaidEle.val(caseableTotalFee - discountApplicable);
        discountRemaining = discountRemaining - discountApplicable;
      }

      updateCaseableDue(casebaleDiscountEle.closest('.caseable_fields'));
      updateReceiptAmount();
    });
  }

  $('#total-discount').on('change paste keyup', function(e){
    if (e.which === 9) return false; // do nothing when keyup event is triggered by tab button
    var totalDiscount = Number($(this).val());
    newDiscountDistribution(totalDiscount);
    updateTotalDue();
  });

  $('#amount-received').on('change paste keyup', function(e){
    if (e.which === 9) return false; // do nothing when keyup event is triggered by tab button
    var amountReceived = Number($(this).val());
    var advance = Number($('#amount-already-paid').text());
    var amountRemaining = amountReceived + advance;
    $('.caseable_fields:not(.hide) .fee_paid').each(function () {
      var casebaleFeePaidEle = $(this);
      var caseableTotalFee = Number(casebaleFeePaidEle.closest('.caseable_fields').find('.total_fee').val());
      var caseableDiscount = Number(casebaleFeePaidEle.closest('.caseable_fields').find('.discount').val());
      var caseableFeePaid = caseableTotalFee - caseableDiscount;
      if (amountRemaining > caseableFeePaid) {
        amountRemaining = amountRemaining - caseableFeePaid;
        casebaleFeePaidEle.val(caseableFeePaid);
      } else {
        casebaleFeePaidEle.val(amountRemaining);
        amountRemaining = 0;
      }
      updateCaseableDue(casebaleFeePaidEle.closest('.caseable_fields'));
    });
    updateTotalDue();
  });

  $('a.accept-due').click(function(e) {
    e.preventDefault();
    var dueAmount = Number($('#total-due').text());
    var amountReceived = Number($('#amount-received').val());
    $('#amount-received').val(amountReceived + dueAmount).trigger('change');
  });

  $('a.calculate-discount').click(function(e) {
    e.preventDefault();
    var discountPercent = Number($('#total-discount').val());
    if (discountPercent < 0) {
      alert('Discount percentage cannot be negative');
    } else if (discountPercent > 100) {
      alert('Discount percentage cannot be more than 100');
    } else {
      var totalAmount = Number($('span#total-amount').text());
      var discountAmount = totalAmount * discountPercent / 100;
      discountAmount = Math.round(discountAmount);
      $('#total-discount').val(discountAmount).trigger('change');
    }
  });

  $('.cancel-caseable').click(function () {
    var casebaleCancelEle = $(this);
    var casebaleDiscountEle = casebaleCancelEle.closest('.caseable_fields').find('.discount');
    var caseableFeePaid = casebaleCancelEle.closest('.caseable_fields').find('.fee_paid');
    casebaleDiscountEle.val(0).trigger('change');
    caseableFeePaid.val(0).trigger('change');
  });
