$(document).on('turbolinks:load', function() {

  function callPoll() {
    if ($('#reg-no-barcode-url-loader').length > 0) {
      var url = window.location.href;
      url = url.replace('#','');
      // Check if url is of bill page
      if (url.indexOf('bill') !== -1) {
        url = url.replace('bill','reg_no_barcode_url');
        pollBarcodeUrl(url);
      }
    }
  }

  function pollBarcodeUrl(url) {
    setTimeout(function() {
      $.ajax({
        url: url,
        type: "GET",
        success: function(data) {
          if (data.reg_no_barcode_url !== null) {
            $('.receipt-barcode span').html( "<img src=' " + data.reg_no_barcode_url + "' />" );
            $('.barcode-reg-no-text').text(data.reg_no);
            $('#reg-no-barcode-url-loader').remove();
          }
        },
        dataType: "json",
        complete: setTimeout(function(){ callPoll(); }, 1000), // Avoids redundant call to server, adds extra 1 sec wait.
        timeout: 15000
      })
    }, 1000);
  }

  callPoll();
});
